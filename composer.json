{
  "name": "gfargo/newsapi",
  "description": "PHP API wrapper for NewsAPI.org",
  "type": "library",
  "keywords": [
    "php",
    "api",
    "wrapper",
    "newsapi",
    "news"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Griffen Fargo",
      "email": "griffen@alley.co"
    }
  ],
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "NewsAPI\\": "src/"
    }
  },
  "require": {
    "php": ">=7.1",
    "rmccue/requests": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.20",
    "squizlabs/php_codesniffer": "3.*"
  }
}
